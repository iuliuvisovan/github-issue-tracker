{
  "expo": {
    "name": "github-issue-tracker",
    "slug": "github-issue-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/ui/assets/images/icon.png",
    "scheme": "github-issue-tracker",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/ui/assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
  }
}
